{
  "name": "bootstrapcdn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./node_modules/",
    "eslint": "eslint .",
    "puglint": "pug-lint views/",
    "pug-lint": "npm run puglint",
    "lint": "npm run eslint && npm run puglint",
    "mocha": "mocha --invert --grep \"functional\"",
    "mocha:suite": "mocha",
    "mocha:functional": "mocha ./test/functional_test.js",
    "dev": "nodemon app.js --ext css,js,pug,svg,png,yml --ignore test",
    "watch": "npm run dev",
    "server": "node app.js",
    "start": "cross-env NODE_ENV=production npm run server",
    "ci": "npm run lint && npm run mocha:suite",
    "test": "npm run lint && npm run mocha",
    "purge-latest": "bash ./scripts/purge.sh",
    "bootlint": "node ./scripts/update_bootlint.js",
    "bootswatch": "node ./scripts/update_bootswatch.js",
    "integrity": "node ./scripts/integrity.js",
    "sri": "node ./scripts/sri.js",
    "wp-plugin": "node ./scripts/wp-plugin.js"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "cross-env": "^5.1.4",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-sitemap": "^1.8.0",
    "express-sslify": "^1.2.0",
    "helmet": "^3.12.0",
    "js-yaml": "^3.11.0",
    "jstransformer-clean-css": "^2.1.0",
    "mime": "^2.3.1",
    "morgan": "^1.9.0",
    "pug": "^2.0.3",
    "rollbar": "^2.3.9",
    "semver": "^5.5.0",
    "serve-favicon": "^2.5.0",
    "sri-toolbox": "^0.2.0",
    "staticify": "^2.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "bootlint": "0.14.2",
    "eslint": "^4.19.1",
    "fs-walk": "^0.0.2",
    "html-validator": "^3.0.0",
    "htmlencode": "^0.0.4",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "pug-lint": "^2.5.0",
    "pug-lint-config-clock": "^2.0.0",
    "request": "^2.85.0",
    "uglify-js": "^3.3.21"
  },
  "engines": {
    "node": "8.x"
  }
}
