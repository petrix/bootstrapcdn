    const bootswatchVersion = semver.coerce(config.bootswatch4.version);
    const bootstrap = config.bootstrap.find((file) => semver.satisfies(file.version, `~${bootswatchVersion}`));
    const jquery = config.javascript.find((file) => file.name === 'jquery');
    const clipboardjs = config.javascript.find((file) => file.name === 'clipboardjs');
    const clipboardjsUri = getVersionedPath(clipboardjs.uri);
    const clipboardjsSri = generateSRI(clipboardjs.uri);
    const mainJs = config.javascript.find((file) => file.name === 'main');
    const mainJsUri = getVersionedPath(mainJs.uri);
    const mainJsSri = generateSRI('/assets/js/main.js');

| var mainJsUri = '#{mainJsUri}';
| var clipboardjsUri = '#{clipboardjsUri}';
| var jqueryUri = '#{jquery.uri}';
| var bootstrapBundleUri = '#{bootstrap.javascriptBundle}';
|
| loadjs([clipboardjsUri, mainJsUri], 'main', {
|    before: function(path, el) {
|        el.integrity = path === mainJsUri ? '#{mainJsSri}' : '#{clipboardjsSri}';
|        el.crossOrigin = 'anonymous';
|     },
|     async: false
| });
| loadjs([jqueryUri, bootstrapBundleUri], 'jquery', {
|    before: function(path, el) {
|        el.integrity = path === jqueryUri ? '#{jquery.sri}' : '#{bootstrap.javascriptBundleSri}';
|        el.crossOrigin = 'anonymous';
|     },
|     async: false
| });
| loadjs.ready('jquery', function() {
|     $('.ads-info-toggler').popover();
| });
